{
    "presets": [
      ["@babel/preset-env", { "modules": false }],
      "@babel/preset-typescript",
      "@babel/preset-react"
    ],
    "plugins": [
      "babel-plugin-styled-components",
      ["@babel/proposal-class-properties", { loose: true }],
      [
        "babel-plugin-inline-import",
        {
            "extensions": [".css"]
        }
      ]
    ]
  }
  
