{
    "extends": "../../.babelrc",
    "plugins": [
      [
        "inline-react-svg",
        {
          "ignorePattern": ".color.svg",
          "svgo": {
            "plugins": [
              { "removeStyleElement": true },
              { "removeTitle": true },
              { "removeDesc": true },
              { "removeAttrs": { "attrs": "class" } }
            ]
          }
        }
      ],
      [
        "inline-react-svg",
        {
          "svgo": {
            "plugins": [{ "removeTitle": true }, { "removeDesc": true }]
          }
        },
        "inline-react-color-svg"
      ]
    ]
  }
  